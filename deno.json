{
  "unstable": [
    "kv",
    "cron"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "esnext",
      "deno.window",
      "deno.unstable"
    ]
  },
  "imports": {
    "$lib": "./src/lib",
    "$lib/": "./src/lib/",
    "@std/ulid": "jsr:@std/ulid@^1.0.0",
    "@std/http": "jsr:@std/http@^1.0.11"
  },
  "include": [
    "./.svelte-kit/ambient.d.ts",
    "./.svelte-kit/non-ambient.d.ts",
    "./.svelte-kit/types/**/$types.d.ts",
    "./vite.config.js",
    "./vite.config.ts",
    "./src/**/*.js",
    "./src/**/*.ts",
    "./src/**/*.svelte",
    "./tests/**/*.js",
    "./tests/**/*.ts",
    "./tests/**/*.svelte"
  ],
  "exclude": [
    "./node_modules/**",
    "./src/service-worker.js",
    "./src/service-worker/**/*.js",
    "./src/service-worker.ts",
    "./src/service-worker/**/*.ts",
    "./src/service-worker.d.ts",
    "./src/service-worker/**/*.d.ts"
  ]
}
